class Solution {
    public int repeatedNTimes(int[] nums) {
        int j=0;
        for(int i=0;i<nums.length;i++){
           j=i+1;
           while(j<nums.length){
               if(nums[i]==nums[j]){
                   return nums[i];
               }
               j++;
           }
        }
        return 0;
    }
}
