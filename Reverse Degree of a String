import java.util.*;
class Solution {
    public int reverseDegree(String s) {
        HashMap<Character,Integer> h=new HashMap<>();
        int n=26,sum=0,index=1;
        char c='a';
        for(;n>0;n--){
            h.put(c,n);
            c++;
        } 
        for(int i=0;i<s.length();i++){
               sum+=h.get(s.charAt(i))*index;
               index++;
        }
        return sum;
    }
}
