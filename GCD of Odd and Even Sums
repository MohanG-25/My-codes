class Solution {
    public static int Gcd(int a,int b){
        if(b==0){return a;}
        
        return Gcd(b,a%b);
        
    }
    public int gcdOfOddEvenSums(int n) {
        int sumOfOddNumbers=n*n;
        int sumOfEvenNumbers=n*(n+1);
        
        return Gcd(sumOfOddNumbers,sumOfEvenNumbers);
    }
}
