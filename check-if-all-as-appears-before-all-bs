class Solution {
    public boolean checkString(String s) {
        if(s.length()==0){
            return false;
        }
        for(int i=1;i<s.length();i++){
            if(s.charAt(i)=='a'){
                if(s.charAt(i-1)!='a'){
                    return false;
                }
            }
        }
        return true;
    }
}
